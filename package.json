{
  "name": "aliina",
  "version": "1.1.0",
  "description": "Spotify playlist generator",
  "main": "src/index.js",
  "scripts": {
    "build": "export NODE_ENV=production && rimraf build/ && webpack --config ./webpack/prod.js",
    "deploy": "npm run build && git checkout gh-pages && git rm *.js && cp ./build/* . && git add *.js && git commit -a -m 'Update bundle'",
    "ci:automerge": "circleci-automerge --filter=\"^greenkeeper/\"",
    "start": "webpack-dev-server --open \"Google Chrome\" --config ./webpack/dev.js",
    "storybook": "start-storybook",
    "test": "npm run test:lint && npm run test:unit",
    "test:e2e": "cd e2e && make && nightwatch && make stop",
    "test:lint": "eslint ./src --format codeframe",
    "test:unit": "jest ./src --coverage",
    "test:update": "jest ./src --updateSnapshot"
  },
  "repository": {
    "type": "git",
    "url": "github.com/lauravuo/aliina"
  },
  "keywords": [
    "spotify",
    "playlist"
  ],
  "homepage": "https://lauravuo.github.io/aliina",
  "author": "lauravuo",
  "license": "MIT",
  "jest": {
    "testRegex": ".*\\.test\\.js$",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.svg$": "jest-svg-transformer"
    }
  },
  "dependencies": {
    "@babel/runtime": "7.5.2",
    "connected-react-router": "6.5.0",
    "history": "4.9.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.1.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4",
    "redux-devtools-extension": "2.13.8",
    "redux-observable": "1.1.0",
    "rxjs": "6.5.2",
    "styled-components": "4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "7.5.2",
    "@babel/plugin-transform-runtime": "7.5.0",
    "@babel/preset-env": "7.5.2",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-storyshots": "5.1.9",
    "@storybook/react": "5.1.9",
    "@svgr/webpack": "4.3.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.6",
    "chromedriver": "75.1.0",
    "circleci-automerge": "1.1.0",
    "config": "^3.2.2",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.8.0",
    "jest-svg-transformer": "1.0.0",
    "nightwatch": "1.1.13",
    "prettier": "1.18.2",
    "react-test-renderer": "16.8.6",
    "rimraf": "2.6.3",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2",
    "webpack-merge": "4.2.1",
    "webpack-version-file": "0.1.3"
  }
}
